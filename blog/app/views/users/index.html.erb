<h1>Listing all users <h1>

<table>


	<tr>
		<td>Email</th>
		<th>Name</th>
		<th colspan="3"></th>
	</tr>


	<% @users.each do |user| %>
		<tr>
			<th><%= user.email%></th>
			<th><%= user.name%></th>
			<th><%= link_to 'Show', user_path(user)%></th>
			<th><%= link_to 'Edit', edit_user_path(user)%></th>
			<th><%= link_to 'Destroy', user_path(user), method: :delete, data: {confirm: 'Seguro?'}%></th>
		</tr>
	<%end %>	
</table>